{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ymovie2",
  "version": "0.1.0",
  "identifier": "com.tauri.ymovie2",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npx cross-env BROWSER=none npm start",
    "beforeBuildCommand": "npm run build"
    
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERCRjQwNUU3Nzk3M0U4NgpSV1NHUHBkM1hrQy9EWUwzTjdmZWhtUE9SZEZZdExwSDdaZDFKZHIreXNUb1REVXp0NGlDUEliTgo=",
      "endpoints": [
        "https://releases.myapp.com/{{target}}/{{arch}}/{{current_version}}",
        
        "https://github.com/user/repo/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "windows": [
      {
        "title": "ymovie2",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  }
}
